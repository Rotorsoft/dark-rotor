    "badge.foreground": "#b4b4b4",
    "button.foreground": "#b4b4b4",
    "checkbox.foreground": "#b4b4b4",
    "dropdown.foreground": "#b4b4b4",
    "editor.foreground": "#b4b4b4",
    "editorLineNumber.activeForeground": "#b4b4b4",
    "foreground": "#b4b4b4",
    "input.foreground": "#b4b4b4",
    "list.activeSelectionForeground": "#b4b4b4",
    "panelSectionHeader.foreground": "#b4b4b4",
    "progressBar.background": "#b4b4b4",
    "sideBar.foreground": "#b4b4b4",
    "tab.activeForeground": "#b4b4b4",
    "terminal.foreground": "#b4b4b4"
